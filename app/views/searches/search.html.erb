<h1>Search for Coffee Shops Near Me</h1>
<%= form_tag '/search' do %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>
  <%= submit_tag "Search!" %>
<% end %>

<div class="">
  <% if @error %>
    <p><%= @error %></p>
  <% elsif @venues %>
    <ul>
      <% @venues.each do |venue| %>
        <%= venue['name'] %><br>
        <%= venue['location']['address'] %><br>
        Checkins: <%= venue['stats']['chekinsCount'] %>
      <% end %>
    </ul>
  <% end %>
</div>
